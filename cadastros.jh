
enum Curso{
	ENGENHARIA
    SISTEMAS
}

entity Aluno{ 
	matricula Integer required
    cpf String
    nomealuno String
    email String
    cursoaluno Curso
}

entity Disciplina{
    codigo Integer required
    nome String
    carga Float
}

entity Professor{
    id Integer required
    nomeprofessor String
}

entity DisciplinaOfertada{
    cod Integer required
}

relationship OneToMany{
    DisciplinaOfertada{disciplina(codigo)} to Disciplina
}

relationship ManyToMany{
	Aluno{disciplinaOfertada(cod)} to DisciplinaOfertada{aluno(matricula)}
}

relationship ManyToOne{
    DisciplinaOfertada{professor(id)} to Professor
}












