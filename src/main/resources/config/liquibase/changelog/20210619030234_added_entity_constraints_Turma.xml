<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Turma.
    -->
    <changeSet id="20210619030234-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="turma_id"
                                 baseTableName="rel_turma__assuntos"
                                 constraintName="fk_rel_turma__assuntos__turma_id"
                                 referencedColumnNames="id"
                                 referencedTableName="turma"/>

        <addForeignKeyConstraint baseColumnNames="assuntos_id"
                                 baseTableName="rel_turma__assuntos"
                                 constraintName="fk_rel_turma__assuntos__assuntos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="assunto"/>

        <addForeignKeyConstraint baseColumnNames="aluno_id"
                                 baseTableName="turma"
                                 constraintName="fk_turma__aluno_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>
    </changeSet>
</databaseChangeLog>
